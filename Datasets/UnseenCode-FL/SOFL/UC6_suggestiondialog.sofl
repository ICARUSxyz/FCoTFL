  this.tabWidget = new QTabWidget(this)
  this.templateListCodeGen = new QListWidget(this)
  this.templateListBugFix = new QListWidget(this)
  this.templateListAnnotation = new QListWidget(this)
  this.editAreaCodeGen = new QTextEdit(this)
  this.editAreaBugFix = new QTextEdit(this)
